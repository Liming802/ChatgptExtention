{
  "manifest_version": 3,
  "name": "Digital sobriety",
  "version": "1.0.0",
  "description": "ChatGPT's analyse",
  "permissions": ["scripting", "cookies", "storage","activeTab","webRequest", "webRequestBlocking", "tabs",  "debugger"],
  "host_permissions": ["<all_urls>"], 
  "action":{},
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  
  "background": {
    "service_worker": "background.js",
     "type": "module"
  },
  "web_accessible_resources": [
  {
    "resources": ["1.png","2.png","3.png","4.png","5.png","6.png","dist/content.bundle.js"],
    "matches": ["<all_urls>"]
  }
]
}
